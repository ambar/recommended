{
  "name": "recommended",
  "version": "0.0.4",
  "description": "Run Prettier/ESLint with `recommended` configs",
  "license": "MIT",
  "repository": "ambar/recommended",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "bin": "cli.js",
  "files": [
    "config",
    "dist",
    "cli.js"
  ],
  "scripts": {
    "prepare": "yarn build",
    "lint": "DEBUG=recommended node cli.js",
    "test": "jest",
    "dev": "yarn build --watch",
    "prebuild": "rm -rf dist",
    "build": "node scripts/esbuild.mjs"
  },
  "jest": {
    "preset": "es-jest",
    "collectCoverageFrom": [
      "src/**"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "debug": "^4.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.1.1",
    "find-cache-dir": "^3.3.2",
    "globby": "^11",
    "jsonc-parser": "^3.0.0",
    "kleur": "^3",
    "mri": "^1.2.0",
    "prettier": "^2.4.1",
    "prompts": "^2.4.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/eslint": "^7.28.0",
    "@types/find-cache-dir": "^3.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/prettier": "^2.3.2",
    "@types/prompts": "^2.0.14",
    "@types/react": "^17.0.24",
    "es-jest": "^1.3.0",
    "esbuild": "^0.13.1",
    "jest": "^27.2.4",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "lint",
    "style",
    "eslint",
    "prettier",
    "typescript",
    "standard",
    "xo"
  ]
}
